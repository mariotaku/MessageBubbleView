<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MessageBubbleView">
        <attr name="cornerRadius" format="dimension" />
        <attr name="bubbleColor" format="color" />
        <attr name="caretWidth" format="dimension" />
        <attr name="caretHeight" format="dimension" />
        <attr name="caretPosition" />
    </declare-styleable>
    <attr name="caretPosition">
        <enum name="none" value="0x0" />
        <enum name="topLeft" value="0x1" />
        <enum name="topRight" value="0x2" />
        <enum name="bottomLeft" value="0x3" />
        <enum name="bottomRight" value="0x4" />
        <enum name="topStart" value="0x11" />
        <enum name="topEnd" value="0x12" />
        <enum name="bottomStart" value="0x13" />
        <enum name="bottomEnd" value="0x14" />
    </attr>
</resources>